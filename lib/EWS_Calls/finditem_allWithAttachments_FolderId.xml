	<t:RequestServerVersion Version="REPLACE_EXCHANGE_VERSION" />
</soap:Header>
<soap:Body>
	<m:FindItem Traversal="Shallow">
		<m:ItemShape>
			<t:BaseShape>IdOnly</t:BaseShape>
		</m:ItemShape>
		<m:IndexedPageItemView MaxEntriesReturned="REPLACE_MAX_ENTRIES" Offset="REPLACE_OFFSET" BasePoint="REPLACE_BASEPOINT" />
		<m:Restriction>
			<t:IsEqualTo>
				<t:FieldURI FieldURI="item:HasAttachments" />
				<t:FieldURIOrConstant>
					<t:Constant Value="true" />
				</t:FieldURIOrConstant>
			</t:IsEqualTo>
		</m:Restriction>
		<m:ParentFolderIds>
			<t:FolderId Id="REPLACE_PARENT_FOLDER_ID" ChangeKey="REPLACE_FOLDER_CHANGE_KEY" />
		</m:ParentFolderIds>
	</m:FindItem>
</soap:Body>
</soap:Envelope>

